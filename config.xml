<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns       = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.bartabat.gmaptest"
        versionCode = "10"
        version     = "1.1.0">

    <name>Gmap test</name>

    <description>
        A simple google map application
    </description>

    <author href="http://www.bartabat.com" email="baptiste.senidre@gmail.com">
        Baptiste SENIDRE
    </author>

    <icon src="img/icons/android/generic.png" gap:platform="android" gap:density="ldpi" />
    <icon src="img/icons/android/generic.png" gap:platform="android" gap:density="mdpi" />
    <icon src="img/icons/android/generic.png" gap:platform="android" gap:density="hdpi" />
    <icon src="img/icons/android/generic.png" gap:platform="android" gap:density="xhdpi" />

    <gap:splash src="img/splash/android/generic.png" gap:platform="android" gap:density="ldpi" />
    <gap:splash src="img/splash/android/generic.png" gap:platform="android" gap:density="mdpi" />
    <gap:splash src="img/splash/android/generic.png" gap:platform="android" gap:density="hdpi" />
    <gap:splash src="img/splash/android/generic.png" gap:platform="android" gap:density="xhdpi" />

    <access origin="*" subdomains="true" />

    <feature name="http://api.phonegap.com/1.0/geolocation"/>
    <feature name="http://api.phonegap.com/1.0/network"/>
    <feature name="http://api.phonegap.com/1.0/notification"/>

</widget>